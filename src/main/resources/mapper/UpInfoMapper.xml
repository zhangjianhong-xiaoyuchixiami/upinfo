<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="org.qydata.mapper.UpInfoMapper">


    <insert id="insertUpInfo" parameterType="String">
        INSERT INTO qysms.SMSUpInfo(msgid,sp_code,src_mobile,msg_content,recv_time,createTime)
        VALUES(#{param1},#{param2},#{param3},#{param4},#{param5},#{param6},nows())
    </insert>

</mapper>